<% if(errors != ''){ %>
    <%errors.forEach((item)=>{%>
     <script>openLoginModal();</script>
     <div style="text-align: center" class="alert alert-danger"><%- item %></div>
    <%});%>
<%}%>
<% if(success != ''){ %>
    <%success.forEach((item)=>{%>
     <script>openLoginModal();</script>
     <div style="text-align: center" class="alert alert-success"><%- item %></div>
    <%});%>
<%}%>
